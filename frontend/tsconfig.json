{
  "include": [
    "src/**/*",
  ],
  "compilerOptions": {
    "target": "es5",
    "module": "ESNext",
    "baseUrl": "./src",
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "strict": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
  },
}